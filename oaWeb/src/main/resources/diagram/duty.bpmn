        <activiti:taskListener event="create" delegateExpression="${dynamicSelection}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="proposer" targetRef="personnel"></sequenceFlow>
    <endEvent id="endevent1" name="End">
